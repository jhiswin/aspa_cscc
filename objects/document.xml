<class name="document" dependent="true">
	<!-- ugly hack to embed MSDOMNode -->
	<class name="MSDOMNode" dependant="true">
		<property name="Value" type="DSTRING">
			<read>
				 <ast name="DOT">
					<this />
					<ast name="IDENTIFIER" text="value" />
				</ast>
			</read>
		</property>
		<method name="focus" type="VOID">
			<read>
				 <ast name="DOT">
					<this />
					<ast name="METHOD_CALL" text="focus" />
				</ast>
			</read>
		</method>
		
		<method name="loadmovie" type="DSTRING">
			<arg type="DINT"/>
			<arg type="DSTRING"/>
			<ast name="DOT">
				<this/>
				<ast name="METHOD_CALL" text="LoadMovie">
					<ast name="ARGLIST_VALUES">
						<arg index="1" />
						<arg index="2" />
					</ast>
				</ast>
			</ast>
		</method>
		<method name="Play" type="VOID">
			<ast name="DOT">
				<this/>
				<ast name="METHOD_CALL" text="Play">
				</ast>
			</ast>
		</method>
		<method name="getAttribute" type="DSTRING">
			<arg type="DSTRING"/>
			<ast name="DOT">
				<this/>
				<ast name="METHOD_CALL" text="getAttribute">
				  <ast name="ARGLIST_VALUES">
					<arg index="1" />
				  </ast>
				</ast>
			</ast>
		</method>
	</class>
	
  <method name="all" type="MSDOMNode">
	<arg type="DSTRING" />
	<ast name="DOT">
		<this/>
		<ast name="METHOD_CALL" text="getElementById">
			<ast name="ARGLIST_VALUES">
				<arg index="1" />
			</ast>
		</ast>
	</ast>
  </method>
  <method name="writeln" type="VOID">
    <arg type="DSTRING" />
    <ast name="DOT">
    	<this/>
    	<ast name="METHOD_CALL" text="writeln">
        	<ast name="ARGLIST_VALUES">
            	<arg index="1" />
        	</ast>
        </ast>
    </ast>
  </method>
  <method name="write" type="VOID">
    <arg type="DSTRING" />
    <ast name="DOT">
    	<this/>
	    <ast name="METHOD_CALL" text="write">
	        <ast name="ARGLIST_VALUES">
	            <arg index="1" />
	        </ast>
	    </ast>
	</ast>
  </method>
</class>

